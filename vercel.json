{
  "version": 2,
  "builds": [
      {
          "src": "src/main.ts",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/api/v1/(.*)",
          "dest": "dist/src/main.js",
          "methods": [
              "GET",
              "POST",
              "PUT",
              "PATCH",
              "DELETE",
              "OPTIONS"
          ]
      }
  ]
}